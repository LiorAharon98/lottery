{
  "expo": {
    "name": "lottery",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },

    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/292e68aa-3480-4c01-b697-6672a8d899fc"
    },
    "slug": "lottery",
    "version": "1.0.30",
    "orientation": "portrait",
    "icon": "./assets/lotto-background.png",
    "userInterfaceStyle": "light",

    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.liors.lottery.first.project"
    },
    "android": {
      "package": "com.liors.lottery.first.project",
      "versionCode": 31,
      "adaptiveIcon": {
        "foregroundImage": "./assets/lotto-background.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },

    "extra": {
      "eas": {
        "projectId": "292e68aa-3480-4c01-b697-6672a8d899fc"
      }
    }
  }
}
